  ${beginTime==void||beginTime.equals("")?"":"AND TO_DATE(VTS.DDATE,'yyyy-mm-dd') &gt;= TO_DATE('"+beginTime+"','yyyy-mm-dd')"}
  ${endTime==void||endTime.equals("")?"":"AND (TO_DATE('"+endTime+"','yyyy-mm-dd'))&gt;= TO_DATE(VTS.DDATE,'yyyy-mm-dd')"}
