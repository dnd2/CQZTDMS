<?xml version="1.0" encoding="UTF-8" standalone="no"?><jatools Class="com.jatools.core.ZReportDocument" Name="jatools report template"><Version>3.11</Version><Variables><Item0 Class="com.jatools.data.rs.sql.ZSqlReader" Name="$$master_dataset"><Name>Day_Report</Name><Sql>SELECT G.GROUP_NAME,
        SUM(CASE
          WHEN R.DATA_TYPE = 'BILL' ${day_==void||"".equals(day_)?"":" AND R.DAY= "+day_}THEN
               R.AMONUT
          ELSE
               0
        END) AS BILL_AMOUNT_DAY,
        SUM(CASE
          WHEN R.DATA_TYPE = 'BILL' ${month_==void||"".equals(month_)?"":" AND R.MONTH= "+month_} THEN
               R.AMONUT
          ELSE
               0
        END) AS BILL_AMOUNT_MONTH,
        SUM(CASE
          WHEN R.DATA_TYPE = 'BILL' ${year_ ==void ||"".equals(year_)?"":" AND R.YEAR ="+year_} THEN
               R.AMONUT
          ELSE
               0
        END) AS BILL_AMOUNT_YEAR,
        SUM(CASE
          WHEN R.DATA_TYPE = 'SALES' ${day_==void||"".equals(day_)?"":" AND R.DAY="+day_} THEN
               R.AMONUT
          ELSE
               0
        END) AS SALES_AMOUNT_DAY,
        SUM(CASE
          WHEN R.DATA_TYPE = 'SALES' ${month_==void||"".equals(month_)?"":" AND R.MONTH= "+month_} THEN
               R.AMONUT
          ELSE
               0
        END) AS SALES_AMOUNT_MONTH,
        SUM(CASE
          WHEN R.DATA_TYPE = 'SALES' ${year_ ==void ||"".equals(year_)?"":" AND R.YEAR ="+year_} THEN
               R.AMONUT
          ELSE
               0
        END) AS SALES_AMOUNT_YEAR
        FROM DAY_CALCULATE_REPORT R, TM_VHCL_MATERIAL_GROUP G
        WHERE R.SERIES_ID = G.GROUP_ID
        ${company_id==void||"".equals(company_id)?"":" AND R.company_id="+company_id}
        GROUP BY G.GROUP_NAME</Sql><Description>test</Description><Connection><Driver>oracle.jdbc.driver.OracleDriver</Driver><User>${p("dbUser")}</User><PasswordX>V7BrDpJDVOA=,bnX8oUXcA09V+G4XsE0x2yNU23Ltnt52</PasswordX><Url>${p("dbUrl")}</Url></Connection></Item0><Item1 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_1"><CalcField>BILL_AMOUNT_DAY</CalcField></Item1><Item2 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_2"><CalcField>BILL_AMOUNT_MONTH</CalcField></Item2><Item3 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_3"><CalcField>BILL_AMOUNT_YEAR</CalcField></Item3><Item4 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_4"><CalcField>SALES_AMOUNT_DAY</CalcField></Item4><Item5 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_5"><CalcField>SALES_AMOUNT_MONTH</CalcField></Item5><Item6 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_6"><CalcField>SALES_AMOUNT_YEAR</CalcField></Item6></Variables><Children><Item0 Class="com.jatools.component.ZSection"><Name>reportheader</Name><X>16</X><Y>16</Y><Height>71</Height><Constraints Class="com.jatools.core.table.ZCell"><Row>0</Row><Col>0</Col><RowSpan>3</RowSpan><ColSpan>0</ColSpan></Constraints></Item0><Item1 Class="com.jatools.component.ZSection"><Name>page header</Name><X>16</X><Y>87</Y><Height>20</Height><Type>1</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>3</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item1><Item2 Class="com.jatools.component.ZSection"><Name>detail</Name><X>16</X><Y>107</Y><Height>20</Height><Type>3</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>4</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item2><Item3 Class="com.jatools.component.ZSection"><Name>report footer</Name><X>16</X><Y>127</Y><Height>60</Height><Type>6</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>5</Row><Col>0</Col><RowSpan>3</RowSpan><ColSpan>0</ColSpan></Constraints></Item3><Item4 Class="com.jatools.component.ZSection"><Name>pagefooter</Name><X>16</X><Y>187</Y><Heig