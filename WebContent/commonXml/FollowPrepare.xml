<?xml version="1.0" encoding="UTF-8" standalone="no"?><jatools Class="com.jatools.core.ZReportDocument" Name="jatools report template"><Version>3.11</Version><Variables><Item0 Class="com.jatools.data.rs.sql.ZSqlReader" Name="$$master_dataset"><Name>aa</Name><Sql>SELECT DISTINCT TMF.FLEET_ID,
                B.ORG_NAME, --大区
                TMR.REGION_NAME, --省 
                TO_CHAR(TMF.SUBMIT_DATE, 'YYYY-MM-DD') SUBMIT_DATE, --报备日期
                TMC.COMPANY_SHORTNAME COMPANY_NAME, --申请 单位
                TMF.FLEET_NAME, --客户名称
                TC.CODE_DESC FLEET_TYPE, --客户类型
                TC1.CODE_DESC MAIN_BUSINESS, --主营业务
                TMF.ZIP_CODE, --邮编
                TMF.ADDRESS, --详细地址
                TMF.MAIN_LINKMAN, --主要联系人
                TMF.MAIN_JOB, --职务
                TMF.MAIN_PHONE, --主要联系人电话
                NVL(TVMG.GROUP_NAME, '全系') GROUP_NAME, --车系
                TMF.SERIES_COUNT, --数量
                TMF.REQ_REMARK, --备注
                TO_CHAR(TFF.FOLLOW_DATE, 'YYYY-MM-DD') FOLLOW_DATE, --跟进日期
                TFF.FOLLOW_REMARK --跟进内容
  FROM TM_FLEET               TMF,
       TM_COMPANY             TMC,
       TM_DEALER              A,
       TM_ORG                 B,
       TM_DEALER_ORG_RELATION C,
       TM_REGION              TMR,
       TM_FLEET_FOLLOW        TFF,
       TM_VHCL_MATERIAL_GROUP TVMG,
       TC_CODE                TC,
       TC_CODE                TC1 
 WHERE TMC.COMPANY_ID = TMF.DLR_COMPANY_ID
   AND TC.CODE_ID = TMF.FLEET_TYPE
   AND TMF.FLEET_ID = TFF.FLEET_ID
   AND TC1.CODE_ID(+)=TMF.MAIN_BUSINESS
   AND TMF.DLR_COMPANY_ID = A.COMPANY_ID
   AND TVMG.GROUP_ID(+) = TMF.SERIES_ID
   AND TMR.REGION_CODE(+) = TMF.REGION
   AND f_get_pid(A.DEALER_ID) = C.DEALER_ID
   AND B.ORG_ID = C.ORG_ID
   AND B.IS_COUNT = 0
   AND A.DEALER_LEVEL = 10851001
   AND TMF.STATUS = 11021003
   ${followDate1==void||followDate1.equals("")?"":"AND TO_DATE(TO_CHAR(TFF.FOLLOW_DATE, 'YYYY-MM-DD'),'yyyy-mm-dd') &gt;=  TO_DATE('"+followDate1+"','yyyy-mm-dd')"}
   ${followDate2==void||followDate2.equals("")?"":"AND (TO_DATE('"+followDate2+"','yyyy-mm-dd')) &gt;=  TO_DATE(TO_CHAR(TFF.FOLLOW_DATE, 'YYYY-MM-DD'),'yyyy-mm-dd')"}
   ${orgId==void||orgId.equals("")?"":" AND B.ORG_ID IN ("+orgId+")"}
   and exists
 (select 1 from tm_fleet_follow ff where ff.fleet_id = tmf.fleet_id)</Sql><Description>aa</Description><Connection><Driver>oracle.jdbc.driver.OracleDriver</Driver><User>${p("dbUser")}</User><PasswordX>00zBPQFov0w=,TPC2Go3J6enU525VLSwjZ1iu6bZml8eX</PasswordX><Url>${p("dbUrl")}</Url></Connection></Item0><Item1 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_1"><CalcField>SERIES_COUNT</CalcField></Item1></Variables><Children><Item0 Class="com.jatools.component.ZSection"><Name>reportheader</Name><X>15</X><Y>15</Y><Height>71</Height><Constraints Class="com.jatools.core.table.ZCell"><Row>0</Row><Col>0</Col><RowSpan>3</RowSpan><ColSpan>0</ColSpan></Constraints></Item0><Item1 Class="com.jatools.component.ZSection"><Name>page header</Name><X>15</X><Y>86</Y><Height>55</Height><Type>1</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>3</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item1><Item2 Class="com.jatools.component.ZSection"><Name>detail</Name><X>15</X><Y>141</Y><Height>75</Height><Type>3</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>4</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item2><Item3 Class="com.jatools.component.ZGroupSection"><Name>group footer(REGION_NAME)</Name><X>15</X><Y>216</Y><Height>20</Height><GroupBy>REGION_NAME</GroupBy><Type>4</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>5</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item3><Item4 Class="com.jatools.component.ZGroupSection"><Name>group footer(ORG_NAME)</Name><X>15</X><Y>236</Y><Height>20</Height><GroupBy>ORG_NAME</GroupBy><Type>4</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>6</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item4><Item5 Class="com.jatools.component.ZSection"><Name>report footer</Name><X>15</X><Y>256</Y><Height>60</Height><Type>6</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>7</Row><Col>0</Col><RowSpan>3</RowSpan><ColSpan>0</ColSpan></Constraints></Item5><Item6 Class="com.jatools.comp