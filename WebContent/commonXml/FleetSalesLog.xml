<?xml version="1.0" encoding="UTF-8" standalone="no"?><jatools Class="com.jatools.core.ZReportDocument" Name="jatools report template"><Version>3.11</Version><Variables><Item0 Class="com.jatools.data.rs.sql.ZSqlReader" Name="$$master_dataset"><Name>777</Name><Sql>SELECT VOD.ROOT_ORG_NAME,
       VOD.REGION_NAME,
       TO_CHAR(TRUNC(TDASL.LOG_DATE),'yyyy-MM-dd'),
       VOD.DEALER_NAME,
       TFC.BUY_FROM,
       TVM.MATERIAL_NAME,
       DECODE(TBA.ERP_CODE,
              726,
              '重庆',
              82,
              '重庆',
              142,
              '河北',
              197,
              '南京') CODE_DESC,
              TFC.CONTRACT_NO,
              TDAS.FLEET_ID,
              TDASL.LOG_STATUS,
              TFC.STATUS,
              count(1)
  FROM TT_DEALER_ACTUAL_SALES     TDAS,
       TT_DEALER_ACTUAL_SALES_LOG TDASL,
       VW_ORG_DEALER              VOD,
       TT_FLEET_CONTRACT          TFC,
       TM_VEHICLE                 TMV,
       TM_VHCL_MATERIAL           TVM,
       TM_BUSINESS_AREA TBA
 WHERE TDAS.ORDER_ID = TDASL.ORDER_ID
   AND VOD.DEALER_ID = TDAS.DEALER_ID
   AND TDAS.IS_FLEET=10041001
   AND TBA.AREA_ID=TMV.AREA_ID
   AND TFC.CONTRACT_NO(+) = TDAS.CONTRACT_NO
   AND TMV.LIFE_CYCLE = 10321004
   AND TMV.VEHICLE_ID = TDAS.VEHICLE_ID
   AND TMV.MATERIAL_ID = TVM.MATERIAL_ID
  -- AND TFC.STATUS IS NOT NULL
  ${orgId==void||orgId.equals("")?"":"AND VOD.ROOT_ORG_ID IN ("+orgId+")"}
              ${produce==void||produce.equals("")?"":" AND DECODE(TBA.ERP_CODE,726,'重庆',82,'重庆',142,'河北',197,'南京')='"+produce+"'"}
              ${checkDate1==void||checkDate1.equals("")?"":"AND TO_DATE(TO_CHAR(TDASL.LOG_DATE,'yyyy-MM-dd'),'yyyy-MM-dd') &gt;= TO_DATE('"+checkDate1+"','yyyy-MM-dd HH24:MI:SS')"}
              ${checkDate2==void||checkDate2.equals("")?"":"AND (TO_DATE('"+checkDate2+"','yyyy-MM-dd HH24:MI:SS'))&gt;= TO_DATE(TO_CHAR(TDASL.LOG_DATE,'yyyy-MM-dd'),'yyyy-MM-dd')"}
              ${dealerId==void||dealerId.equals("")?"":" AND VOD.DEALER_ID IN ("+dealerId+")"}
GROUP BY VOD.ROOT_ORG_NAME,
       VOD.REGION_NAME,
       TRUNC(TDASL.LOG_DATE),
       VOD.DEALER_NAME,TDAS.FLEET_ID,TFC.STATUS,
       TFC.BUY_FROM,TBA.ERP_CODE,TDASL.LOG_STATUS,
       TVM.MATERIAL_NAME,TFC.CONTRACT_NO</Sql><Description>777</Description><Connection><Driver>oracle.jdbc.driver.OracleDriver</Driver><User>${p("dbUser")}</User><PasswordX>p/6J4OAgiSU=,1NN3sScmkafGp1TTWgdOyrcRf54Q/EWJ</PasswordX><Url>${p("dbUrl")}</Url></Connection></Item0><Item1 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_1"><CalcField>COUNT(1)</CalcField><GroupField>REGION_NAME</GroupField></Item1><Item2 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_2"><CalcField>COUNT(1)</CalcField><GroupField>ROOT_ORG_NAME</GroupField></Item2><Item3 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_3"><CalcField>COUNT(1)</CalcField></Item3></Variables><Children><Item0 Class="com.jatools.component.ZSection"><Name>reportheader</Name><X>16</X><Y>16</Y><Height>71</Height><Constraints Class="com.jatools.core.table.ZCell"><Row>0</Row><Col>0</Col><RowSpan>3</RowSpan><ColSpan>0</ColSpan></Constraints></Item0><Item1 Class="com.jatools.component.ZSection"><Name>page header</Name><X>16</X><Y>87</Y><Height>40</Height><Type>1</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>3</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item1><Item2 Class="com.jatools.component.ZSection"><Name>detail</Name><X>16</X><Y>127</Y><Height>24</Height><Type>3</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>4</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item2><Item3 Class="com.jatools.component.ZGroupSection"><Name>group footer(REGION_NAME)</Name><X>16</X><Y>151</Y><Height>20</Height><GroupBy>REGION_NAME</GroupBy><Type>4</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>5</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item3><Item4 Class="com.jatools.component.ZGroupSection"><Name>group footer(ROOT_ORG_NAME)</Name><X>16</X><Y>171</Y><Height>20</Height><GroupBy>ROOT_ORG_NAME</GroupBy><Type>4</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>6</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item4><Item5 Class="com.jatools.component.ZSe