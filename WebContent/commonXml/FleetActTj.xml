<?xml version="1.0" encoding="UTF-8" standalone="no"?><jatools Class="com.jatools.core.ZReportDocument" Name="jatools report template"><Version>3.11</Version><Variables><Item0 Class="com.jatools.data.rs.sql.ZSqlReader" Name="$$master_dataset"><Name>444</Name><Sql>SELECT       XX.DEALER_CODE, --经销商代码
               XX.ROOT_ORG_NAME, --大区
               XX.ROOT_DEALER_NAME,--上级单位
               XX.ROOT_DEALER_ID, --上级单位
               XX.REGION_NAME, --省份
               XX.CITY_NAME, --城市
               XX.TOWN, --省份
               XX.AREA_NAME, --业务范围
               TO_CHAR(XX.SALES_DATE,'YYYY-MM-DD'), --销售日期
               TO_CHAR(XX.PRODUCT_DATE,'YYYY-MM-DD'), --生产日期
               TO_CHAR(XX.INVOICE_DATE,'YYYY-MM-DD'), --开票日期
               XX.DEALER_SHORTNAME, --销售单位
               XX.CTM_NAME, --客户名称
               XX.CTM_TYPE, --客户类型
               XX.IS_FLEET, --是否集团客户
               XX.FLEET_NAME, --集团客户名称
               XX.CONTRACT_NO, --集团客户合同
               XX.SERNAME, --车型系列
               XX.MODNAME, --车型类别
               XX.MODCODE, --车型编码
               XX.PACKNAME, --状态
               XX.PARTFLEET_TYPE,
               XX.FLEET_TYPE,
               XX.PARTFLEET_TYPE,
               XX.COLOR,
               XX.VIN
  FROM (SELECT TMD.DEALER_CODE, --经销商代码
               VOD.ROOT_ORG_NAME, --大区
               VOD.ROOT_DEALER_NAME,--上级单位
               VOD.ROOT_DEALER_ID, --上级单位
               VOD.REGION_NAME, --省份
               VOD.CITY_NAME, --城市
               TMR.REGION_NAME TOWN, --省份
               TBA.AREA_NAME, --业务范围
               TTS.SALES_DATE, --销售日期
               TMV.PRODUCT_DATE, --生产日期
               TTS.INVOICE_DATE, --开票日期
               TMD.DEALER_SHORTNAME, --销售单位
               TTC.CTM_NAME, --客户名称
               TC1.CODE_DESC CTM_TYPE, --客户类型
               TTS.IS_FLEET, --是否集团客户
               TF.FLEET_NAME, --集团客户名称
               TFC.CONTRACT_NO, --集团客户合同
               TVMG.GROUP_NAME SERNAME, --车型系列
               TVMG1.GROUP_NAME MODNAME, --车型类别
               TVMG1.GROUP_CODE MODCODE, --车型编码
               TVMG2.GROUP_NAME PACKNAME, --状态
               TC2.CODE_DESC PARTFLEET_TYPE,
               TC2.CODE_DESC FLEET_TYPE,
               TMV.COLOR,
               TMV.VIN
          FROM TT_DEALER_ACTUAL_SALES TTS,
               TM_DEALER              TMD,
               TT_CUSTOMER            TTC,
               TM_VEHICLE             TMV,
               TM_VHCL_MATERIAL       M,
               TM_BUSINESS_AREA       TBA,
               TM_FLEET               TF,
               TT_FLEET_CONTRACT      TFC,
               VW_ORG_DEALER          VOD,
               TM_VHCL_MATERIAL_GROUP TVMG,
               TM_VHCL_MATERIAL_GROUP TVMG1,
               TM_VHCL_MATERIAL_GROUP TVMG2,
               TC_CODE TC1,
               TC_CODE TC2,
               TM_REGION TMR
         WHERE TTS.DEALER_ID = TMD.DEALER_ID
           AND TC2.CODE_ID=TF.FLEET_TYPE
           AND TC1.CODE_ID=TTC.CTM_TYPE
           AND TVMG.GROUP_ID = TMV.SERIES_ID
           AND TVMG1.GROUP_ID = TMV.MODEL_ID
           AND TVMG2.GROUP_ID = TMV.PACKAGE_ID
           AND TBA.AREA_ID(+) = TMV.AREA_ID
           AND VOD.DEALER_ID = TMD.DEALER_ID
           AND TTS.CTM_ID = TTC.CTM_ID
           AND TMR.REGION_CODE=TTC.TOWN
           AND TTS.VEHICLE_ID = TMV.VEHICLE_ID
           AND TMV.MATERIAL_ID = M.MATERIAL_ID
           AND TTS.FLEET_ID = TF.FLEET_ID(+)
           AND TTS.IS_RETURN = '10041002'
           AND TTS.IS_FLEET = 10041001
           AND TTS.CONTRACT_ID = TFC.CONTRACT_ID(+)
           AND TMV.LIFE_CYCLE = 10321004
           ${contractNo==void||contractNo.equals("")?"":" AND TFC.CONTRACT_NO ='"+contractNo+"'"}
   ${fleetName==void||fleetName.equals("")?"":" AND TTC.CTM_NAME ='"+fleetName+"'"}
   ${fleetType==void||fleetType.equals("")?"":" AND TF.FLEET_TYPE ='"+fleetType+"'"}
   ${vin==void||vin.equals("")?"":" AND TMV.VIN LIKE '%"+vin+"%'"}
   ${groupCode==void||groupCode.equals("")?"":" AND TVMG.GROUP_CODE='"+groupCode+"'"}
   ${companyId==void||companyId.equals("")?"":" AND TTS.DLR_COMPANY_ID IN("+companyId+")"}
   ${checkStatus==void||checkStatus.equals("")?"":" AND TTS.FLEET_STATUS ='"+checkStatus+"'"}
   ${startDate==void||startDate.equals("")?"":"AND TTS.SALES_DATE &gt;= TO_DATE('"+startDate+"','yyyy-mm-dd')"}
   ${endDate==void||endDate.equals("")?"":"AND (TO_DATE('"+endDate+"','yyyy-mm-dd'))&gt;= TTS.SALES_DATE"}
   
        UNION
        SELECT DISTINCT  TMD.DEALER_CODE, --经销商代码
               VOD.ROOT_ORG_NAME, --大区
               VOD.ROOT_DEALER_NAME,--上级单位
               VOD.ROOT_DEALER_ID, --上级单位
               VOD.REGION_NAME, --省份
               VOD.CITY_NAME, --城市
               TMR.REGION_NAME TOWN, --省份
               TBA.AREA_NAME, --业务范围
               TTS.SALES_DATE, --销售日期
               TMV.PRODUCT_DATE, --生产日期
               TTS.INVOICE_DATE, --开票日期
               TMD.DEALER_SHORTNAME, --销售单位
               TTC.CTM_NAME, --客户名称
               TC2.CODE_DESC CTM_TYPE, --客户类型
               TTS.IS_FLEET, --是否集团客户
               TCP.PACT_NAME FLEET_NAME, --集团客户名称
               TFC.CONTRACT_NO CONTRACT_NO, --集团客户合同
               TVMG.GROUP_NAME SERNAME, --车型系列
               TVMG1.GROUP_NAME MODNAME, --车型类别
               TVMG1.GROUP_CODE MODCODE, --车型编码
               TVMG2.GROUP_NAME PACKNAME, --状态
               TC3.CODE_DESC PARTFLEET_TYPE,
               TCP.PACT_NAME FLEET_TYPE,
               TMV.COLOR,
               TMV.VIN
          FROM TT_DEALER_ACTUAL_SALES TTS,
               TM_DEALER              TMD,
               TT_CUSTOMER            TTC,
               TM_VEHICLE             TMV,
               TM_VHCL_MATERIAL       M,
               TM_FLEET               TF,
               TT_FLEET_CONTRACT      TFC,
               TM_COMPANY_PACT        TCP,
               VW_ORG_DEALER          VOD,
               TM_BUSINESS_AREA       TBA,
               TM_VHCL_MATERIAL_GROUP TVMG,
               TM_VHCL_MATERIAL_GROUP TVMG1,
               TM_VHCL_MATERIAL_GROUP TVMG2,
               TC_CODE TC2,
               TC_CODE TC3,
               TM_REGION TMR
         WHERE  TTS.DEALER_ID = TMD.DEALER_ID
           AND TMR.REGION_CODE=TTC.TOWN
           AND TC3.CODE_ID=TCP.PARENT_FLEET_TYPE
           AND TC2.CODE_ID=TTC.CTM_TYPE
           AND TVMG.GROUP_ID = TMV.SERIES_ID
           AND TBA.AREA_ID(+) = TMV.AREA_ID
           AND TVMG1.GROUP_ID = TMV.MODEL_ID
           AND TVMG2.GROUP_ID = TMV.PACKAGE_ID
           AND VOD.DEALER_ID = TMD.DEALER_ID
           AND TTS.CTM_ID = TTC.CTM_ID
           AND TTS.VEHICLE_ID = TMV.VEHICLE_ID
           AND TMV.MATERIAL_ID = M.MATERIAL_ID
           AND TTS.FLEET_ID = TCP.PACT_ID
           AND TTS.IS_RETURN = '10041002'
           AND TTS.IS_FLEET = 10041001
           AND TTS.CONTRACT_ID = TFC.CONTRACT_ID(+)
           AND TMV.LIFE_CYCLE = 10321004
           ${contractNo==void||contractNo.equals("")?"":" AND TFC.CONTRACT_NO ='"+contractNo+"'"}
   ${fleetName==void||fleetName.equals("")?"":" AND TTC.CTM_NAME ='"+fleetName+"'"}
   ${fleetType==void||fleetType.equals("")?"":" AND TC3.CODE_ID ='"+fleetType+"'"}
   ${vin==void||vin.equals("")?"":" AND TMV.VIN LIKE '%"+vin+"%'"}
   ${groupCode==void||groupCode.equals("")?"":" AND TVMG.GROUP_CODE='"+groupCode+"'"}
   ${companyId==void||companyId.equals("")?"":" AND TTS.DLR_COMPANY_ID IN("+companyId+")"}
   ${checkStatus==void||checkStatus.equals("")?"":" AND TTS.FLEET_STATUS ='"+checkStatus+"'"}
   ${startDate==void||startDate.equals("")?"":"AND TTS.SALES_DATE &gt;= TO_DATE('"+startDate+"','yyyy-mm-dd')"}
   ${endDate==void||endDate.equals("")?"":"AND (TO_DATE('"+endDate+"','yyyy-mm-dd'))&gt;= TTS.SALES_DATE"}
   
           ) XX
 WHERE (XX.IS_FLEET=10041001 AND XX.FLEET_NAME IS NOT NULL) OR XX.IS_FLEET=10041002
 </Sql><Description>444</Description><Connection><Driver>oracle.jdbc.driver.OracleDriver</Driver><User>${p("dbUser")}</User><PasswordX>fMi/kkCt5Tg=,/pYIXngW5ekanVs+VEi1YtA4glYt2ISb</PasswordX><Url>${p("dbUrl")}</Url></Connection></Item0></Variables><Children><Item0 Class="com.jatools.component.ZSection"><Name>reportheader</Name><X>16</X><Y>16</Y><Height>71</Height><Constraints Class="com.jatools.core.table.ZCell"><Row>0</Row><Col>0</Col><RowSpan>3</RowSpan><ColSpan>0</ColSpan></Constraints></Item0><Item1 Class="com.jatools.component.ZSection"><Name>page header</Name><X>16</X><Y>87</Y><Height>31</Height><Type>1</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>3</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item1><Item2 Class="com.jatools.component.ZSection"><Name>detail</Name><X>16</X><Y>118</Y><Height>20</Height><Type>3</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>4</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item2><Item3 Class="com.jatools.component.ZGroupSection"><Name>group footer(REGION_NAME)</Name><X>16</X><Y>138</Y><Height>20</Height><GroupBy>REGION_NAME</GroupBy><Type>4</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>5</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item3><Item4 Class="com.jatools.component.ZGroupSection"><Name>group footer(ROOT_ORG_NAME)</Name><X>16</X><Y>158</Y><Height>20</Height><GroupBy>ROOT_ORG_NAME</GroupBy><Type>4</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>6</Row><Col>0</Col><ColSpan>0</ColSpan></Constraints></Item4><Item5 Class="com.jatools.component.ZSection"><Name>report footer</Name><X>16</X><Y>178</Y><Height>60</Height><Type>6</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>7</Row><Col>0</Col><RowSpan>3</RowSpan><ColSpan>0</ColSpan></Constraints></Item5><Item6 Class="com.jatools.component.ZSection"><Name>pagefooter</Name><X>16</X><Y>238</Y><Height>20</Height><Type>5</Type><Constraints Class="com.jatools.core.table.ZCell"><Row>10</Row><Col>