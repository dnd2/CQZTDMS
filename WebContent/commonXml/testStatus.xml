<?xml version="1.0" encoding="UTF-8" standalone="no"?><jatools Class="com.jatools.core.ZReportDocument" Name="jatools report template"><Version>3.11</Version><Variables><Item0 Class="com.jatools.data.rs.sql.ZSqlReader" Name="$$master_dataset"><Name>test1</Name><Sql>WITH 
X AS
 (SELECT O.COMPANY_ID,
         VDO.ROOT_ORG_ID AS ORG_ID,
         VDO.ROOT_ORG_CODE AS ORG_CODE,
         VDO.ROOT_ORG_NAME AS ORG_NAME,
         VDO.REGION_NAME,
         VDO.ROOT_DEALER_CODE,
         VDO.ROOT_DEALER_NAME,
         VDO.DEALER_ID,
         VDO.DEALER_CODE,
         VDO.DEALER_NAME,
         (SUM(NVL(DT.DELIVERY_AMOUNT, 0)) - SUM(NVL(DT.OUT_AMOUNT, 0))) AS NODE_BILL_AMOUNT
    FROM TT_VS_DLVRY D, TT_VS_DLVRY_DTL DT, VW_ORG_DEALER VDO, TT_VS_ORDER O 
   WHERE D.DELIVERY_ID = DT.DELIVERY_ID
     AND O.ORDER_ID = D.ORDER_ID
     AND D.DELIVERY_STATUS IN (10281004) --已开票
     AND O.ORDER_ORG_ID = VDO.DEALER_ID --按订货方来进行统计
     ${areaId==void||areaId.equals("")?"":" AND O.AREA_ID IN  ("+areaId+")"}
     ${dealerId==void||dealerId.equals("")?"":" AND VDO.DEALER_ID IN  ("+dealerId+")"}
     ${orgId==void||orgId.equals("")?"":" AND VDO.ROOT_ORG_ID IN  ("+orgId+")"}
    ${modelId==void||modelId.equals("")?"":"AND EXISTS (SELECT 1 FROM VW_MATERIAL_GROUP VW, TM_VHCL_MATERIAL_GROUP_R GR  WHERE VW.PACKAGE_ID = GR.GROUP_ID  AND GR.MATERIAL_ID = VW.MODEL_ID AND (VW.BRAND_ID IN ("+modelId+") OR VW.SERIES_ID IN ("+modelId+") OR VW.MODEL_ID IN ("+modelId+") OR VW.PACKAGE_ID IN ("+modelId+")))"}
     
   GROUP BY O.COMPANY_ID,
            VDO.ROOT_ORG_ID,
            VDO.ROOT_ORG_CODE,
            VDO.ROOT_ORG_NAME,
            VDO.REGION_NAME,
            VDO.ROOT_DEALER_ID,
            VDO.ROOT_DEALER_CODE,
            VDO.ROOT_DEALER_NAME,
            VDO.DEALER_ID,
            VDO.DEALER_CODE,
            VDO.DEALER_NAME),

Y AS
 (SELECT VOD.COMPANY_ID,
         VOD.ROOT_ORG_ID AS ORG_ID,
         VOD.ROOT_ORG_CODE AS ORG_CODE,
         VOD.ROOT_ORG_NAME AS ORG_NAME,
         VOD.REGION_NAME,
         VOD.ROOT_DEALER_CODE,
         VOD.ROOT_DEALER_NAME,
         VOD.DEALER_ID,
         VOD.DEALER_CODE,
         VOD.DEALER_NAME,
         SUM(CASE
               WHEN LIFE_CYCLE = 10321005 THEN
                1
               ELSE
                0
             END) DEING_AMOUNT, --在途
         SUM(CASE
               WHEN LIFE_CYCLE = 10321003 THEN
                1
               ELSE
                0
             END) STOCK_AMOUNT --在库
    FROM TM_VEHICLE V, VW_ORG_DEALER VOD ,VW_MATERIAL_GROUP VW, TM_VHCL_MATERIAL_GROUP_R GR
   WHERE VOD.DEALER_ID = V.DEALER_ID
     AND V.DEALER_ID &gt; 0
     AND VW.PACKAGE_ID = GR.GROUP_ID
     AND GR.MATERIAL_ID = V.MATERIAL_ID
     AND GR.GROUP_ID IN--业务范围过滤
         (SELECT T1.GROUP_ID
            FROM TM_VHCL_MATERIAL_GROUP T1
           WHERE T1.STATUS = 10011001
           START WITH T1.GROUP_ID IN
                      (SELECT TAP.MATERIAL_GROUP_ID
                         FROM TM_AREA_GROUP TAP
                        WHERE 1=1
                        ${areaId==void||areaId.equals("")?"":" AND TAP.AREA_ID IN  ("+areaId+")"}
                         )
          CONNECT BY PRIOR T1.GROUP_ID = T1.PARENT_GROUP_ID)
   ${modelId==void||modelId.equals("")?"":"AND EXISTS (SELECT 1 FROM VW_MATERIAL_GROUP VW, TM_VHCL_MATERIAL_GROUP_R GR  WHERE VW.PACKAGE_ID = GR.GROUP_ID  AND GR.MATERIAL_ID = V.MATERIAL_ID AND (VW.BRAND_ID IN ("+modelId+") OR VW.SERIES_ID IN ("+modelId+") OR VW.MODEL_ID IN ("+modelId+") OR VW.PACKAGE_ID IN ("+modelId+")))"}
     ${dealerId==void||dealerId.equals("")?"":" AND vod.dealer_id IN  ("+dealerId+")"}
     ${orgId==void||orgId.equals("")?"":" AND vod.ROOT_ORG_ID IN  ("+orgId+")"}
     ${modelId==void||modelId.equals("")?"":" AND (VW.BRAND_ID IN ("+modelId+") OR VW.SERIES_ID IN ("+modelId+") OR VW.MODEL_ID IN ("+modelId+") OR VW.PACKAGE_ID IN ("+modelId+"))"}
   GROUP BY VOD.COMPANY_ID,
            VOD.ROOT_ORG_ID,
            VOD.ROOT_ORG_CODE,
            VOD.ROOT_ORG_NAME,
            VOD.REGION_NAME,
            VOD.ROOT_DEALER_CODE,
            VOD.ROOT_DEALER_NAME,
            VOD.DEALER_ID,
            VOD.DEALER_CODE,
            VOD.DEALER_NAME)
SELECT NVL(X.COMPANY_ID, Y.COMPANY_ID) COMPANY_ID,
       NVL(X.ORG_ID, Y.ORG_ID) ORG_ID,
       NVL(X.ORG_CODE, Y.ORG_CODE) ORG_CODE,
       NVL(X.ORG_NAME, Y.ORG_NAME) ORG_NAME,
       NVL(X.REGION_NAME, Y.REGION_NAME) REGION_NAME,
       NVL(X.ROOT_DEALER_CODE, Y.ROOT_DEALER_CODE) ROOT_DEALER_CODE,
       NVL(X.ROOT_DEALER_NAME, Y.ROOT_DEALER_NAME) ROOT_DEALER_NAME,
       NVL(X.DEALER_CODE, Y.DEALER_CODE) DEALER_CODE,
       NVL(X.DEALER_NAME, Y.DEALER_NAME) DEALER_NAME,
       NVL(X.NODE_BILL_AMOUNT, 0) NODE_BILL_AMOUNT,
       NVL(Y.DEING_AMOUNT, 0) DEING_AMOUNT,
       NVL(Y.STOCK_AMOUNT, 0) STOCK_AMOUNT
  FROM  X FULL OUTER JOIN Y
    ON X.DEALER_ID = Y.DEALER_ID
ORDER BY COMPANY_ID,ORG_CODE,REGION_NAME,ROOT_DEALER_CODE,DEALER_CODE
</Sql><Description>test1</Description><Connection><Driver>oracle.jdbc.driver.OracleDriver</Driver><User>${p("dbUser")}</User><PasswordX>9+Yj6dVM6oU=,W6WP9iJqHxD5aOuA38gU5c3Y+Rx6vzca</PasswordX><Url>${p("dbUrl")}</Url></Connection></Item0><Item1 Class="com.jatools.data.ZParameter" Name="areaId"><Name>areaId</Name><Type1>java.lang.String</Type1><DefaultValue>2010010100000002</DefaultValue><Prompt>areaId</Prompt></Item1><Item2 Class="com.jatools.data.ZParameter" Name="dealerId"><Name>dealerId</Name><Type1>java.lang.String</Type1><DefaultValue>1</DefaultValue><Prompt>11</Prompt></Item2><Item3 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_1"><CalcField>NODE_BILL_AMOUNT</CalcField><GroupField>REGION_NAME</GroupField></Item3><Item4 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_2"><CalcField>DEING_AMOUNT</CalcField><GroupField>REGION_NAME</GroupField></Item4><Item5 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_3"><CalcField>STOCK_AMOUNT</CalcField><GroupField>REGION_NAME</GroupField></Item5><Item6 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_4"><CalcField>NODE_BILL_AMOUNT</CalcField><GroupField>ORG_NAME</GroupField></Item6><Item7 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_5"><CalcField>DEING_AMOUNT</CalcField><GroupField>ORG_NAME</GroupField></Item7><Item8 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_6"><CalcField>STOCK_AMOUNT</CalcField><GroupField>ORG_NAME</GroupField></Item8><Item9 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_7"><CalcField>NODE_BILL_AMOUNT</CalcField></Item9><Item10 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_8"><CalcField>DEING_AMOUNT</CalcField></Item10><Item11 Class="com.jatools.data.fun.ZGroupSum" Name="$sum_9"><CalcField>STOCK_AMOUNT</CalcField></Item11><Item12 Class="com.jatools.data.ZFormula" Name="$formula_1"><Formula>toLong(""+$NODE_BILL_AMOUNT)+toLong(""+$DEING_AMOUNT)+toLong(""+$STOCK_AMOUNT)</Formula></Item12><Item13 Class="com.jatools.data.ZFormula" Name="$formula_2"><Formula>toLong(""+$sum_1)+ toLong(